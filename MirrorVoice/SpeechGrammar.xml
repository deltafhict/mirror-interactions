<grammar version="1.0" xml:lang="en-US" root="rootRule" tag-format="semantics/1.0-literals" xmlns="http://www.w3.org/2001/06/grammar">
  <rule id="rootRule">
    <one-of>
      <item>
        <tag>AGENDA</tag>
        <item>
          <ruleref uri="#Commands"/> agenda
        </item>
      </item>
      <item>
        <tag>INITIALIZE FACE</tag>
        <item>
          initialize face
        </item>
      </item>
    </one-of>
  </rule>
  <rule id="Commands">
    <item>
      <one-of>
        <item>Open</item>
        <item>Close</item>
      </one-of>
    </item>
  </rule>
</grammar>